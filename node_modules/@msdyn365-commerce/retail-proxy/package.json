{
    "name": "@msdyn365-commerce/retail-proxy",
    "version": "9.24.5",
    "description": "",
    "main": "dist/index.js",
    "module": "dist/index.es5.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "build": "yarn clean && yarn tsc -p tsconfig.json",
        "build:prod": "yarn build",
        "clean": "yarn rimraf dist",
        "debug-tests": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --collectCoverage=false",
        "format": "yarn prettier es5 src/**/**.{ts,tsx}",
        "format:fix": "yarn prettier es5 src/**/**.{ts,tsx} --write",
        "lint": "yarn tslint src/**/*.{ts,tsx} --project tsconfig.json --format stylish",
        "lint:fix": "yarn tslint src/**/*.{ts,tsx} --project tsconfig.json --fix --format stylish",
        "precommit": "lint-staged",
        "start": "yarn clean && yarn tsc -p tsconfig.json --watch",
        "test": "yarn jest",
        "test:ci": "yarn jest --ci"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/jest": "^24.0.11",
        "@types/node": "^10.12.9",
        "jest": "^24.7.1",
        "jest-junit": "^6.3.0",
        "lint-staged": "^8.1.5",
        "prettier": "^1.15.3",
        "rimraf": "^2.6.2",
        "ts-jest": "^24.0.2",
        "tslint": "^5.16.0",
        "typescript": "^3.4.3",
        "typemoq": "^2.1.0"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "mobx": "^4.9.2",
        "tslib": "^1.9.3"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "yarn format:fix",
            "yarn lint:fix",
            "git add"
        ]
    },
    "jest": {
        "preset": "./jest-preset.js"
    }
}