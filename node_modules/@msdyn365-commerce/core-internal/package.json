{
    "name": "@msdyn365-commerce/core-internal",
    "version": "1.27.15",
    "scripts": {
        "build": "yarn tsc -p tsconfig.json && yarn copy-assets",
        "build:prod": "yarn build",
        "clean": "yarn rimraf dist",
        "debug-tests": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --collectCoverage=false",
        "format": "yarn prettier es5 src/**/**.{ts,tsx}",
        "format:fix": "yarn pretty-quick --staged --pattern \"**/**.*(js|jsx|ts|tsx|json|css|scss|md)\"",
        "lint": "yarn tslint src/**/*.{ts,tsx} --project tsconfig.json --format stylish",
        "lint:fix": "yarn tslint src/**/*.{ts,tsx} --project tsconfig.json --fix --format stylish",
        "precommit": "lint-staged",
        "start": "yarn tsc -p tsconfig.json --watch",
        "test": "yarn jest",
        "test:ci": "yarn jest --ci",
        "copy-assets": "node copy-assets.js"
    },
    "author": "",
    "dependencies": {
        "@msdyn365-commerce/telemetry-internal": "1.27.15",
        "axios": "^0.18.0",
        "dompurify": "^2.1",
        "express": "^4.16.3",
        "intl": "^1.2.5",
        "intl-format-cache": "^2.1.0",
        "intl-locales-supported": "^1.0.0",
        "locale-currency": "^0.0.2",
        "lodash": "^4.17.11",
        "mobx": "^4.9.2",
        "mobx-react": "^5.4.0",
        "react": "16.9.0",
        "react-contenteditable": "3.3.3",
        "react-dom": "16.9.0",
        "react-side-effect": "^1.1.1",
        "tslib": "^1.9.3"
    },
    "description": "",
    "devDependencies": {
        "@msdyn365-commerce/retail-proxy": "9.16.10",
        "@types/dompurify": "^2.0.3",
        "@msdyn365-commerce/tslint-rules": "1.27.15",
        "@types/express": "^4.16.0",
        "@types/intl": "^1.2.0",
        "@types/intl-locales-supported": "^1.0.0",
        "@types/jest": "^24.0.11",
        "@types/lodash": "^4.14.119",
        "@types/node": "^10.12.9",
        "@types/react": "16.9.0",
        "@types/react-dom": "16.0.11",
        "@types/react-side-effect": "~1.1.1",
        "@types/sinon": "^7.0.3",
        "@types/uuid": "^3.4.4",
        "@types/winston": "^2.4.4",
        "jest": "^24.7.1",
        "jest-junit": "^6.3.0",
        "lint-staged": "^8.1.5",
        "prettier": "^1.15.3",
        "pretty-quick": "^1.8.0",
        "rimraf": "^2.6.2",
        "sinon": "^7.2.2",
        "ts-jest": "^24.0.2",
        "tslint": "^5.16.0",
        "typescript": "^3.4.3"
    },
    "peerDependencies": {
        "@msdyn365-commerce/retail-proxy": "^9.16.8"
    },
    "engines": {
        "node": ">= 10.x.x"
    },
    "files": [
        "dist"
    ],
    "jest": {
        "preset": "../cli-internal/jest-preset"
    },
    "license": "ISC",
    "lint-staged": {
        "*.{ts,tsx}": [
            "yarn format:fix",
            "yarn lint:fix",
            "git add"
        ]
    },
    "main": "dist/lib/index.js",
    "module": "dist/index.es5.js",
    "typings": "dist/types/index.d.ts"
}