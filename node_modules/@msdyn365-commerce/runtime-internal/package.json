{
    "name": "@msdyn365-commerce/runtime-internal",
    "version": "1.27.15",
    "scripts": {
        "analyze": "yarn cross-env ANALYZE_BUNDLE=true webpack --mode=development --config=webpack.config.ts --display-optimization-bailout",
        "analyze:prod": "yarn cross-env ANALYZE_BUNDLE=true webpack --mode=production --config=webpack.config.ts --display-optimization-bailout",
        "build": "yarn tsc -p ./tsconfig.json",
        "build:prod": "yarn tsc -p ./tsconfig.json",
        "clean": "yarn rimraf dist",
        "debug-tests": "node --inspect-brk ../../node_modules/jest/bin/jest.js --runInBand --collectCoverage=false",
        "format": "yarn prettier **/**.{ts,tsx}",
        "format:fix": "yarn pretty-quick --staged --pattern \"**/**.*(js|jsx|ts|tsx|json|css|scss|md)\"",
        "lint": "yarn tslint src/**/*.{ts,tsx} --project tsconfig.json --format stylish",
        "lint:fix": "yarn tslint src/**/*.{ts,tsx} --project tsconfig.json --fix --format stylish",
        "precommit": "lint-staged",
        "start": "yarn tsc -p ./tsconfig.json --watch",
        "test": "yarn jest",
        "test:ci": "yarn jest --ci"
    },
    "author": "",
    "dependencies": {
        "@msdyn365-commerce-modules/core-components": "1.27.15",
        "@msdyn365-commerce/action-internal": "1.27.15",
        "@msdyn365-commerce/cache-internal": "1.27.15",
        "@msdyn365-commerce/core-internal": "1.27.15",
        "@msdyn365-commerce/telemetry-internal": "1.27.15",
        "@msdyn365-commerce/theming-internal": "1.27.15",
        "@msdyn365-commerce/utilities-internal": "1.27.15",
        "@types/tunnel": "^0.0.0",
        "axios": "^0.18.0",
        "body-parser": "^1.18.3",
        "bootstrap-icons": "1.0.0-alpha4",
        "combined-stream": "^1.0.7",
        "compression": "^1.7.2",
        "cookie-session": "^1.3.3",
        "cross-env": "^5.2.0",
        "express": "^4.16.3",
        "fast-json-stringify": "^1.16.4",
        "fs-extra": "^7.0.1",
        "jsonpointer": "^4.0.1",
        "jwt-decode": "^3.0.0",
        "lazysizes": "^5.2.2",
        "lodash": "^4.17.11",
        "lodash.clonedeep": "^4.5.0",
        "long": "^4.0.0",
        "mobx": "^4.9.2",
        "mobx-react": "^5.4.0",
        "react": "16.9.0",
