        "node": ">= 10.x.x"
    },
    "files": [
        "dist"
    ],
    "jest": {
        "preset": "../cli-internal/jest-preset",
        "setupFiles": [
            "jest-localstorage-mock"
        ]
    },
    "license": "ISC",
    "lint-staged": {
        "linters": {
            "*.{ts,tsx}": [
                "yarn format:fix",
                "yarn lint:fix",
                "git add"
            ]
        }
    },
    "main": "dist/server.js"
