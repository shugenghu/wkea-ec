        "@types/combined-stream": "^1.0.1",
        "@types/express": "^4.16.0",
        "@types/fs-extra": "^5.0.4",
        "@types/glob": "^7.1.1",
        "@types/jest": "^24.0.11",
        "@types/jwt-decode": "^2.2.1",
        "@types/lodash": "^4.14.119",
        "@types/mock-fs": "^3.6.30",
        "@types/node": "^10.12.9",
        "@types/react": "16.9.0",
        "@types/react-dom": "16.9.0",
        "jest": "^24.7.1",
        "jest-express": "^1.10.0",
        "jest-junit": "^6.3.0",
