{
    "name": "@msdyn365-commerce/action-internal",
    "version": "1.27.15",
    "scripts": {
        "build": "yarn tsc -p tsconfig.json",
        "build:prod": "yarn build",
        "clean": "yarn rimraf dist",
        "format:fix": "yarn pretty-quick --staged --pattern \"**/**.*(js|jsx|ts|tsx|json|css|scss|md)\"",
        "lint": "yarn tslint src/**/*.{ts,tsx} --project tsconfig.json --format stylish",
        "lint:fix": "yarn tslint src/**/*.{ts,tsx} --project tsconfig.json --fix --format stylish",
        "precommit": "lint-staged",
        "start": "yarn tsc -p tsconfig.json --watch",
        "test": "yarn jest",
        "test:ci": "yarn jest --ci",
        "test:live": "yarn jest --watch"
    },
    "author": "",
    "dependencies": {
        "@msdyn365-commerce/telemetry-internal": "1.27.15",
        "@msdyn365-commerce/cache-internal": "1.27.15",
        "@msdyn365-commerce/core-internal": "1.27.15",
        "clean-webpack-plugin": "^1.0.0",
        "html-webpack-plugin": "^4.0.0-alpha.2",
        "lodash": "^4.17.11",
        "mobx": "^4.9.2",
        "mobx-react": "^5.4.0",
        "react": "16.9.0",
        "react-dom": "16.9.0",
        "ts-loader": "^5.3.0",
        "tslib": "^1.9.3",
        "uuid": "^3.3.3",
        "webpack": "^4.26.0",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.10"
    },
    "description": "",
    "devDependencies": {
        "@msdyn365-commerce/retail-proxy": "9.16.10",
        "@msdyn365-commerce/tslint-rules": "1.27.15",
        "@types/jest": "^24.0.11",
        "@types/lodash": "^4.14.119",
        "@types/node": "^10.12.9",
        "@types/react": "16.9.0",
        "@types/react-dom": "16.9.0",
        "jest": "^24.7.1",
        "jest-extended": "^0.11.5",
        "jest-junit": "^6.3.0",
        "lint-staged": "^8.1.5",
        "nodemon": "^1.18.6",
        "prettier": "^1.15.3",
        "pretty-quick": "^1.8.0",
        "reflect-metadata": "^0.1.12",
        "rimraf": "^2.6.2",
        "ts-jest": "^24.0.2",
        "ts-node": "^7.0.1",
        "tslib": "^1.9.3",
        "tslint": "^5.16.0",
        "tslint-react": "3.6.0",
        "typescript": "^3.4.3"
    },
    "peerDependencies": {
        "@msdyn365-commerce/retail-proxy": "^9.16.8"
    },
    "engines": {
        "node": ">= 10.x.x"
    },
    "files": [
        "dist"
    ],
    "jest": {
        "preset": "../cli-internal/jest-preset",
        "setupFilesAfterEnv": [
            "jest-extended"
        ]
    },
    "keywords": [],
    "license": "ISC",
    "lint-staged": {
        "*.{ts,tsx}": [
            "yarn format:fix",
            "yarn lint:fix",
            "git add"
        ]
    },
    "main": "dist/lib/index.js",
    "module": "dist/index.es5.js",
    "typings": "dist/types/index.d.ts"
}