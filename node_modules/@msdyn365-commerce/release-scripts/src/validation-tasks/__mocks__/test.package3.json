{
    "scripts": {
        "analyze": "yarn cross-env ANALYZE_BUNDLE=true msdyn365b start",
        "analyze:prod": "yarn cross-env ANALYZE_BUNDLE=true msdyn365b build",
        "add-module": "yarn msdyn365 add-module module-created-by-cli",
        "build": "yarn msdyn365b build",
        "build:prod": "yarn build",
        "clean": "yarn rimraf build lib dist .tmp gendef.lock",
        "debug": "yarn msdyn365b debug",
        "format": "yarn prettier **/**.{ts,tsx}",
        "format:fix": "yarn pretty-quick --staged --pattern \"**/**.*(js|jsx|ts|tsx|json|css|scss|md)\"",
        "lint": "yarn tslint {src,test}/**/*.{ts,tsx} --project tsconfig.json --format stylish",
        "lint:fix": "yarn tslint {src,test}/**/*.{ts,tsx} --project tsconfig.json --fix --format stylish",
        "precommit": "lint-staged",
        "postinstall": "yarn validate-dependencies validate",
        "run-integration": "yarn npm-run-all add-module integration",
        "start": "yarn msdyn365b start local",
        "start:prod": "yarn cross-env NODE_ENV=production node build/server.js"
    },
