{
    "name": "@msdyn365-commerce/cache-internal",
    "version": "1.27.15",
    "description": "Internal package that houses app-cache, request-cache and client-cache code",
    "main": "dist/lib/index.js",
    "module": "dist/index.es5.js",
    "typings": "dist/types/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "build:prod": "yarn build",
        "clean": "rimraf dist",
        "format": "prettier es5 src/**/**.{ts,tsx}",
        "format:fix": "yarn pretty-quick --staged --pattern \"**/**.*(js|jsx|ts|tsx|json|css|scss|md)\"",
        "lint": "tslint src/**/*.{ts,tsx} --project tsconfig.json --format stylish",
        "lint:fix": "tslint src/**/*.{ts,tsx} --project tsconfig.json --fix --format stylish",
        "precommit": "lint-staged",
        "start": "tsc -p tsconfig.json --watch",
        "test": "yarn jest",
        "test:ci": "yarn jest"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/express": "^4.16.0",
        "@types/uuid": "^3.4.4",
        "@types/jest": "^24.0.11",
        "@types/node": "^10.7.0",
        "jest": "^24.7.1",
        "jest-junit": "^6.3.0",
        "lint-staged": "^8.1.5",
        "prettier": "^1.17.0",
        "pretty-quick": "^1.8.0",
        "@msdyn365-commerce/retail-proxy": "9.16.10",
        "@msdyn365-commerce/tslint-rules": "1.27.15",
        "rimraf": "^2.6.2",
        "ts-jest": "^24.0.2",
        "tslint": "^5.16.0",
        "typescript": "^3.4.3"
    },
    "dependencies": {
        "@msdyn365-commerce/core-internal": "1.27.15",
        "@msdyn365-commerce/telemetry-internal": "1.27.15",
        "react": "16.9.0",
        "tslib": "1.9.3",
        "node-cache": "4.2.0",
        "mobx": "^4.9.2",
        "uuid": "3.3.2"
    },
    "peerDependencies": {
        "@msdyn365-commerce/retail-proxy": "^9.16.8"
    },
    "jest": {
        "preset": "../cli-internal/jest-preset.js"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "yarn format:fix",
            "yarn lint:fix",
            "git add"
        ]
    }
}