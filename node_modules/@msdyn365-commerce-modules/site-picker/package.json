{
    "name": "@msdyn365-commerce-modules/site-picker",
    "version": "9.24.14",
    "scripts": {
        "build": "yarn build-package",
        "build:prod": "yarn build",
        "clean": "yarn rimraf build dist lib",
        "debug": "yarn msdyn365b debug",
        "debug-tests": "yarn msdyn365b debug-tests",
        "format": "yarn prettier es5 src/**/**.{ts,tsx}",
        "format:fix": "yarn prettier es5 src/**/**.{ts,tsx} --write",
        "lint": "yarn lint-package",
        "lint:fix": "yarn lint-package --fix",
        "precommit": "lint-staged",
        "start": "yarn msdyn365b start",
        "test": "yarn jest",
        "test:ci": "yarn jest --ci"
    },
    "dependencies": {
        "@msdyn365-commerce/bootloader": "1.22.13",
        "@msdyn365-commerce-modules/utilities": "9.24.14",
        "tslib": "^1.9.3"
    },
    "devDependencies": {
        "@types/jest": "^24.0.9",
        "@types/react": "^16.4.10",
        "@types/react-dom": "16.9.0",
        "@types/react-test-renderer": "16.9.0",
        "jest": "^24.1.0",
        "jest-junit": "^6.3.0",
        "lint-staged": "^7.3.0",
        "prettier": "^1.15.2",
        "rimraf": "^2.6.2",
        "ts-jest": "^24.0.0",
        "tslint": "^5.16.0",
        "tslint-react": "^4.0.0"
    },
    "engines": {
        "node": ">= 10.x.x"
    },
    "files": [
        "dist",
        "src"
    ],
    "jest": {
        "preset": "@msdyn365-commerce/cli-internal"
    },
    "license": "SEE LICENSE IN LICENSE.md",
    "lint-staged": {
        "*.{ts,tsx}": [
            "yarn format:fix",
            "yarn lint:fix",
            "git add"
        ]
    },
    "main": "dist/lib/index.js",
    "module": "dist/index.es5.js",
    "typings": "dist/types/index.d.ts"
}