      "value": "Configuration"
    },
    "salesLineQuantityText": {
      "comment": "Sales line quantity label",
      "value": "Qty"
    },
    "discountStringText": {
      "comment": "String to show discount offer string",
      "value": "Savings "
    },
    "posChannelNameText": {
      "comment": "String to show POS channel name",
      "value": "Purchased at "
