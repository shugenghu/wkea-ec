      "value": "Ready for pickup"
    },
    "orderStatusProcessing": {
      "comment": "Order status for order processing",
      "value": "Processing"
    },
    "orderStatusShipped": {
      "comment": "Order status for order shipped",
      "value": "Shipped"
    },
    "orderStatusPickedUp": {
      "comment": "Order status for order picked up",
      "value": "Picked up"
