{
    "name": "@msdyn365-commerce-modules/checkout",
    "version": "9.24.14",
    "license": "SEE LICENSE IN LICENSE.md",
    "main": "dist/lib/index.js",
    "module": "dist/index.es5.js",
    "typings": "dist/types/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "build": "yarn build-package",
        "build:prod": "yarn build",
        "debug": "yarn start --inspect-brk",
        "debug-tests": "node --inspect-brk ../../node_modules/jest/bin/jest.js --runInBand --collectCoverage=false",
        "clean": "yarn rimraf build dist lib",
        "format": "yarn prettier es5 src/**/**.{ts,tsx}",
        "format:fix": "yarn prettier es5 src/**/**.{ts,tsx} --write",
        "lint": "yarn tslint src/**/*.{ts,tsx} --project tsconfig.json --format stylish",
        "lint:fix": "yarn tslint src/**/*.{ts,tsx} --project tsconfig.json --fix --format stylish",
        "prebuild": "yarn clean",
        "precommit": "lint-staged",
        "prestart": "yarn clean",
        "scss-hack": "node ..\\..\\scripts\\build-scss.js",
        "start": "yarn msdyn365b start local",
        "start:prod": "npm-run-all set-node server",
        "set-node": "SET NODE_ENV=production",
        "server": "node build/server.js",
        "test": "yarn jest",
        "test:ci": "yarn jest --ci"
    },
    "devDependencies": {
        "@types/enzyme": "^3.1.14",
        "@types/jest": "24.0.9",
        "@types/react": "16.9.0",
        "@types/react-dom": "16.9.0",
        "@types/react-test-renderer": "^16.0.2",
        "babel-plugin-universal-import": "^3.1.2",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "enzyme-to-json": "^3.3.4",
        "jest-junit": "^6.3.0",
        "jest": "24.1.0",
        "lint-staged": "7.3.0",
        "prettier": "^1.15.2",
        "react-test-renderer": "^16.4.2",
        "rimraf": "^2.6.2",
        "testcafe": "^1.0.0",
        "ts-jest": "24.0.0",
        "tslint": "^5.11.0",
        "tslint-react": "4.0.0",
        "typescript": "^3.2.1"
    },
    "dependencies": {
        "@msdyn365-commerce/commerce-entities": "9.24.14",
        "@msdyn365-commerce/components": "9.24.14",
        "@msdyn365-commerce/global-state": "9.24.14",
        "@msdyn365-commerce-modules/checkout-utilities": "9.24.14",
        "@msdyn365-commerce-modules/data-types": "9.24.14",
        "@msdyn365-commerce-modules/order-summary-utilities": "9.24.14",
        "@msdyn365-commerce-modules/retail-actions": "9.24.14",
        "@msdyn365-commerce-modules/utilities": "9.24.14",
        "@msdyn365-commerce/bootloader": "^1.0.0",
        "identity-obj-proxy": "^3.0.0",
        "tslib": "^1.9.3"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "yarn format:fix",
            "yarn lint:fix",
            "git add"
        ]
    },
    "jest": {
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!**/__tests__/**",
            "!**/tests/**",
            "!**/mocks/**",
            "!**/*.data.ts",
            "!**/*.style.ts",
            "!**/*.props.autogenerated.ts",
            "!**/*.definition.json",
            "!src/modules/checkout-delivery-options/**",
            "!src/modules/checkout-loyalty/**",
            "!src/modules/checkout-payment-instrument/**"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 70,
                "functions": 70,
                "lines": 70,
                "statements": 70
            }
        },
        "preset": "@msdyn365-commerce/cli-internal",
        "setupFiles": [
            "<rootDir>/src/jest-setup.js"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ]
    }
}