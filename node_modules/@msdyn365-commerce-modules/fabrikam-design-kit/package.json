{
    "name": "@msdyn365-commerce-modules/fabrikam-design-kit",
    "version": "9.24.16",
    "scripts": {
        "build": "yarn build-package",
        "build:prod": "yarn build",
        "clean": "yarn rimraf build dist lib",
        "debug": "yarn msdyn365b debug",
        "debug-tests": "yarn msdyn365b debug-tests",
        "format": "yarn prettier es5 src/**/**.{ts,tsx}",
        "format:fix": "yarn prettier es5 src/**/**.{ts,tsx} --write",
        "lint": "yarn lint-package",
        "lint:fix": "yarn lint-package --fix",
        "precommit": "lint-staged",
        "start": "yarn msdyn365b start",
        "test": "yarn jest",
        "test:ci": "yarn jest --ci",
        "scss-hack": "node ..\\..\\scripts\\build-scss.js"
    },
    "dependencies": {
        "@msdyn365-commerce/components": "9.24.16",
        "@msdyn365-commerce-modules/buybox": "9.24.16",
        "@msdyn365-commerce-modules/cart": "9.24.16",
        "@msdyn365-commerce-modules/content-block": "9.24.16",
        "@msdyn365-commerce-modules/promo-banner": "9.24.16",
        "@msdyn365-commerce-modules/ratings-reviews": "9.24.16",
        "@msdyn365-commerce-modules/text-block": "9.24.16",
        "bootstrap": "^4.3.1",
        "identity-obj-proxy": "^3.0.0",
        "tslib": "^1.9.3"
    },
    "devDependencies": {
        "@types/jest": "24.0.9",
        "@types/react": "16.9.0",
        "@types/react-dom": "16.9.0",
        "@types/react-test-renderer": "16.9.0",
        "babel-plugin-universal-import": "^3.1.2",
        "jest": "24.1.0",
        "jest-junit": "^6.3.0",
        "lint-staged": "7.3.0",
        "prettier": "^1.15.2",
        "react-test-renderer": "16.9.0",
        "rimraf": "^2.6.2",
        "testcafe": "^1.0.0",
        "testcafe-reporter-junit": "^3.0.0",
        "ts-jest": "24.0.0",
        "tslint": "^5.16.0",
        "tslint-react": "4.0.0",
        "typescript": "^3.4.3"
    },
    "files": [
        "dist",
        "src"
    ],
    "jest": {
        "preset": "@msdyn365-commerce/cli-internal"
    },
    "license": "SEE LICENSE IN LICENSE.md",
    "lint-staged": {
        "*.{ts,tsx}": [
            "yarn format:fix",
            "yarn lint:fix",
            "git add"
        ]
    },
    "main": "dist/lib/index.js",
    "module": "dist/index.es5.js",
    "typings": "dist/types/index.d.ts"
}