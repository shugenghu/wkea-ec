/*
* THIS FILE IS AUTO-GENERATED AT BUILD TIME - MANUAL MODIFICATIONS WILL BE LOST
*/
{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "declarationDir": "dist/types",
    "allowUnreachableCode": false,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "inlineSources": true,
    "jsx": "react",
    "lib": ["es6", "es2015", "es2017", "dom"],
    "module": "esnext",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,      
    "noUnusedLocals": true,
    "outDir": "dist/lib",
    "pretty": true,
    "resolveJsonModule": true,
    "rootDir": "./src",
    "skipLibCheck": true,
    "sourceMap": true,
    "sourceRoot": "./src",
    "strict": true,    
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "esnext",
    "removeComments": true
  },
  "exclude": [
    "acceptance-tests",
    "build",
    "coverage",
    "dist",
    "jest",
    "jest.config.js",
    "lib",
    "node_modules",
    "prettify.js",
    "scripts",
    "tests",
    "webpack"
  ],
  "types": ["typePatches", "node", "webpack-env"],
  "typeRoots": [
    "node_modules/@types"
  ]
}