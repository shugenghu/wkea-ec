{
    "name": "@msdyn365-commerce-modules/automation-utilities",
    "version": "9.24.16",
    "scripts": {
        "build": "npm-run-all tsc-build copy-mocks",
        "build:prod": "npm-run-all tsc-build copy-mocks",
        "tsc-build": "yarn tsc -p ./tsconfig.json",
        "copy-mocks-yarn": "yarn copy-mocks",
        "clean": "yarn rimraf dist",
        "format": "yarn prettier **/**.{ts,tsx}",
        "format:fix": "yarn prettier **/**.{ts,tsx} --write",
        "lint": "yarn tslint src/**/*.{ts,tsx} --project tsconfig.json --format stylish",
        "lint:fix": "yarn tslint src/**/*.{ts,tsx} --project tsconfig.json --fix --format stylish",
        "precommit": "lint-staged",
        "rebuild": "npm-run-all clean build",
        "start": "npm-run-all copy-mocks-yarn tsc-watch",
        "tsc-watch": "yarn tsc -p ./tsconfig.json --watch",
        "debug-tests": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --collectCoverage=false",
        "copy-mocks": "yarn copyfiles -f -s ./src/*.js ./src/environment-config.json dist/lib"
    },
    "dependencies": {
        "@msdyn365-commerce/commerce-entities": "9.24.16",
        "@msdyn365-commerce-modules/data-types": "9.24.16",
        "@msdyn365-commerce-modules/retail-actions": "9.24.16",
        "@msdyn365-commerce/bootloader": "^1.0.0",
        "@types/js-cookie": "^2.2.0",
        "axios": "^0.18.0",
        "fs-extra": "^8.0.1",
        "js-cookie": "^2.2.0",
        "react": "16.9.0",
        "tslib": "^1.9.3",
        "universal-cookie": "^3.0.4"
    },
    "devDependencies": {
        "@msdyn365-commerce/tslint-rules": "^1.0.0",
        "@types/jest": "24.0.9",
        "@types/react": "16.9.0",
        "@types/react-dom": "16.9.0",
        "@types/react-test-renderer": "^16.0.2",
        "babel-plugin-universal-import": "^3.1.2",
        "axe-core": "3.5.5",
        "axe-sarif-converter": "2.3.1",
        "jest": "24.1.0",
        "jest-junit": "^6.3.0",
        "lint-staged": "7.3.0",
        "prettier": "^1.15.2",
        "react-test-renderer": "^16.4.2",
        "rimraf": "^2.6.2",
        "testcafe": "^1.0.0",
        "testcafe-reporter-junit": "^3.0.0",
        "ts-jest": "24.0.0",
        "tslint": "^5.11.0",
        "tslint-react": "4.0.0",
        "typescript": "^3.2.1"
    },
    "engines": {
        "node": ">= 10.x.x"
    },
    "files": [
        "dist",
        "src"
    ],
    "jest": {
        "preset": "@msdyn365-commerce/cli-internal"
    },
    "license": "SEE LICENSE IN LICENSE.md",
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "yarn format:fix",
            "yarn lint:fix",
            "git add"
        ]
    },
    "main": "dist/lib/index.js",
    "module": "dist/index.es5.js",
    "typings": "dist/types/index.d.ts"
}