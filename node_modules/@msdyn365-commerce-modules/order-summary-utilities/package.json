{
    "name": "@msdyn365-commerce-modules/order-summary-utilities",
    "version": "9.24.14",
    "license": "Copyright (c) Microsoft Corporation. All rights reserved. Licensed under the MIT License. See License.txt in the project root for license information.",
    "main": "dist/lib/index.js",
    "typings": "dist/types/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "build": "npm-run-all tsc-build",
        "build:prod": "npm-run-all tsc-build",
        "tsc-build": "yarn tsc -p ./tsconfig.json",
        "clean": "yarn rimraf dist",
        "format": "yarn prettier **/**.{ts,tsx}",
        "format:fix": "yarn prettier **/**.{ts,tsx} --write",
        "lint": "yarn tslint src/**/*.{ts,tsx} --project tsconfig.json --format stylish",
        "lint:fix": "yarn tslint src/**/*.{ts,tsx} --project tsconfig.json --fix --format stylish",
        "precommit": "lint-staged",
        "rebuild": "npm-run-all clean build",
        "start": "npm-run-all tsc-watch",
        "tsc-watch": "yarn tsc -p ./tsconfig.json --watch",
        "test": "yarn jest",
        "test:ci": "yarn jest --ci",
        "debug-tests": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --collectCoverage=false"
    },
    "dependencies": {
        "@msdyn365-commerce/commerce-entities": "9.24.14",
        "@msdyn365-commerce/components": "9.24.14",
        "@msdyn365-commerce-modules/data-types": "9.24.14",
        "@msdyn365-commerce/bootloader": "^1.0.0",
        "@types/classnames": "^2.2.6",
        "@types/enzyme": "^3.1.14",
        "@types/js-cookie": "^2.2.0",
        "axios": "^0.18.0",
        "js-cookie": "^2.2.0",
        "react": "16.9.0",
        "react-dom": "16.9.0",
        "react-html-parser": "^2.0.2",
        "react-popper": "0.10.4",
        "tslib": "^1.9.3",
        "universal-cookie": "^3.0.4"
    },
    "devDependencies": {
        "@types/jest": "24.0.9",
        "@types/jsdom": "^12.2.0",
        "@types/react": "16.9.0",
        "@types/react-transition-group": "^2.9.2",
        "@types/enzyme-adapter-react-16": "^1.0.3",
        "chalk": "^2.4.1",
        "cookie-parser": "^1.4.3",
        "css-mqpacker": "^7.0.0",
        "ejs": "^2.6.1",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "enzyme-to-json": "^3.3.4",
        "fs-extra": "^8.0.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "24.1.0",
        "jest-css-modules-transform": "^2.0.0",
        "jest-junit": "^6.3.0",
        "loglevel": "^1.6.1",
        "nodegit": "^0.25.1",
        "npm-run-all": "^4.1.3",
        "prettier": "^1.15.2",
        "react-test-renderer": "^16.4.2",
        "react-transition-group": "^4.1.1",
        "rimraf": "^2.6.3",
        "ts-jest": "24.0.0",
        "tslint": "^5.11.0",
        "typescript": "^3.2.1"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "yarn format:fix",
            "yarn lint:fix",
            "git add"
        ]
    },
    "jest": {
        "collectCoverage": true,
        "coverageThreshold": {
            "global": {
                "branches": 75,
                "functions": 75,
                "lines": 75,
                "statements": 75
            }
        },
        "preset": "@msdyn365-commerce/cli-internal",
        "setupFiles": [
            "<rootDir>/src/jest-setup.js"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "coverageReporters": [
            "text",
            "lcov",
            "cobertura"
        ],
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!src/**/index.ts"
        ]
    }
}