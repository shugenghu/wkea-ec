{
    "name": "@msdyn365-commerce-modules/iframe",
    "version": "9.24.16",
    "scripts": {
        "build": "yarn build-package",
        "build:prod": "yarn build",
        "clean": "yarn rimraf build dist lib",
        "debug": "yarn msdyn365b debug",
        "debug-tests": "yarn msdyn365b debug-tests",
        "format": "yarn prettier es5 src/**/**.{ts,tsx}",
        "format:fix": "yarn prettier es5 src/**/**.{ts,tsx} --write",
        "lint": "yarn lint-package",
        "lint:fix": "yarn lint-package --fix",
        "precommit": "lint-staged",
        "start": "yarn msdyn365b start",
        "test": "yarn jest",
        "test:ci": "yarn jest --ci"
    },
    "dependencies": {
        "@msdyn365-commerce/bootloader": "^1.0.0",
        "@msdyn365-commerce-modules/utilities": "9.24.16",
        "tslib": "^1.9.3"
    },
    "devDependencies": {
        "@types/enzyme": "^3.1.14",
        "@types/jest": "24.0.9",
        "@types/react": "^16.4.10",
        "@types/react-dom": "16.9.0",
        "@types/react-test-renderer": "16.9.0",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "enzyme-to-json": "^3.3.4",
        "jest": "24.1.0",
        "jest-junit": "^6.3.0",
        "lint-staged": "7.3.0",
        "prettier": "^1.15.2",
        "rimraf": "^2.6.2",
        "ts-jest": "24.0.0",
        "tslint": "^5.16.0",
        "tslint-react": "4.0.0"
    },
    "engines": {
        "node": ">= 10.x.x"
    },
    "files": [
        "dist",
        "src"
    ],
    "jest": {
        "collectCoverage": true,
        "coverageThreshold": {
            "global": {
                "branches": 80,
                "functions": 80,
                "lines": 80,
                "statements": 80
            }
        },
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!src/index.ts",
            "!**/__tests__/**",
            "!**/*.props.autogenerated.ts"
        ],
        "preset": "@msdyn365-commerce/cli-internal",
        "setupFiles": [
            "<rootDir>/src/jest-setup.js"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ]
    },
    "license": "SEE LICENSE IN LICENSE.md",
    "lint-staged": {
        "*.{ts,tsx}": [
            "yarn format:fix",
            "yarn lint:fix",
            "git add"
        ]
    },
    "main": "dist/lib/index.js",
    "module": "dist/index.es5.js",
    "typings": "dist/types/index.d.ts"
}